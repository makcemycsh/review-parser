{
  "printWidth": 120,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^@core/(.*)$",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^../(.*)$",
    "^(.*)/__internal/(.*)$",
    "^(.*).tsx",
    "^(.*).svg",
    "^(.*).(s)css"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
